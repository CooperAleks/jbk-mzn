section.footer
	.l
		.footer__in
			.footer__soc
				ul.social
					-icons = ['fb', 'tw', 'is']
					-links = ['https://www.facebook.com/onukisound/', 'https://twitter.com/ONUKI_TECH', 'https://www.instagram.com/onukisound/']
